{
    "manifestVersion": 1,
    "id": "Drop-plan-extension-test",
    "version": "1.1.33",
    "name": "Sprint Drop Plan-test",
    "description": "Plan and track your sprint with a calendar based view.",
    "publisher": "yanivsegev",
    "public": false,
    "icons": {
        "default": "images/logo.png"
    },
    "tags": [
        "Plan",
        "Sprint",
        "Backlog",
        "Collaborate"
    ],
    "categories": [
        "Plan and track"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/yanivsegev/VSTS-DropPlan/issues"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yanivsegev/VSTS-DropPlan"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "demands": [
        "api-version/3.0"
    ],
    "contributions": [
        {
            "id": "DropPlanTest",
            "type": "ms.vss-web.tab",
            "description": "Adds Drop Plan to the Work hub group",
            "targets": [
                "ms.vss-work-web.iteration-backlog-tabs"
            ],
            "properties": {
                "name": "Drop plan test",
                "order": 99,
                "uri": "index.html"
            }
        },
        {
            "id": "CreateTasks",
            "type": "ms.vss-web.action",
            "description": "Create tasks for drop plan",
            "targets": [
                "ms.vss-work-web.work-item-context-menu"
            ],
            "properties": {
                "text": "Create dev and qa tasks",
                "title": "Create dev and qa tasks",
                "toolbarText": "Create dev and qa tasks",                
                "icon": "images/logo.png",
                "uri": "toolbar.html",
                "registeredObjectId": "CreateTasks"
            }
        }
    ],
    "files": [
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "scripts",
            "addressable": true
        },
        {
            "path": "styles",
            "addressable": true
        },
        {
            "path": "images",
            "addressable": true
        }
    ],
    "scopes": [
        "vso.work",
        "vso.work_write"
    ]
}