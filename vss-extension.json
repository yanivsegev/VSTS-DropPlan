{
    "manifestVersion": 1,
    "id": "Drop-plan-extension#{testing-flag}",
    "version": "2.1.12#{beta-flag}",
    "name": "Sprint Drop Plan#{testing-flag}",
    "description": "Plan and track your sprint with a calendar based view.",
    "publisher": "yanivsegev",
    "public": false,
    "icons": {
        "default": "images/logo.png"
    },
    "tags": [
        "Plan",
        "Sprint",
        "Backlog",
        "Collaborate"
    ],
    "categories": [
        "Azure Boards"
    ],
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        },
        "privacypolicy": {
          "path": "PrivacyPolicy.md"
        }
    },
    "links": {
        "support": {
            "uri": "https://github.com/yanivsegev/VSTS-DropPlan/issues"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/yanivsegev/VSTS-DropPlan"
    },
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "demands": [
        "api-version/3.0"
    ],
    "contributions": [
        {
            "id": "DropPlan#{testing-flag}",
            "type": "ms.vss-web.tab",
            "description": "Adds Drop Plan to the Work hub group",
            "targets": [
                "ms.vss-work-web.iteration-backlog-tabs"
            ],
            "properties": {
                "name": "Drop Plan#{testing-flag}",
                "order": 99,
                "uri": "index.html"
            }
        },
        {
            "id": "DropPlan-settings-hub#{testing-flag}",
            "type": "ms.vss-web.hub",
            "targets": [
                "ms.vss-web.project-admin-hub-group"
            ],
            "properties": {
                "name": "Drop Plan Settings#{testing-flag}",
                "order": 10,
                "uri": "dropPlan-settings.html",
                "icon": "images/logo.png"
            }
        },
        {
            "id": "dropPlan-workItemGroup#{testing-flag}",
            "type": "ms.vss-work-web.work-item-form-group",
            "targets": [
                "ms.vss-work-web.work-item-form"
            ],
            "properties": {
                "name": "Drop Plan #{testing-flag}",
                "uri": "dropPlan-workItemGroup.html",
                "height": 16
            }
        }
    ],
    "files": [
        {
            "path": "index.html",
            "addressable": true
        },
        {
            "path": "scripts",
            "addressable": true
        },
        {
            "path": "styles",
            "addressable": true
        },
        {
            "path": "images",
            "addressable": true
        },
        {
            "path": "dropPlan-settings.html",
            "addressable": true
        },
        {
            "path": "dropPlan-workItemGroup.html",
            "addressable": true
        }
    ],
    "scopes": [
        "vso.work",
        "vso.work_write"
    ]
}
